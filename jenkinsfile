pipeline{
    agent any
    stages{
        stage('build'){
            steps{
                echo 'build complete'
            }
        }
         stage('test'){
            steps{
                echo 'test complete'
            }
        }
         stage('deploy'){
            steps{
                echo 'deploy complete'
            }
        }
         stage('checkout'){
            steps{
               git url: 'https://github.com/SKK402/demo' , branch: 'main'
            }
        }
    }
}
